    <!--Vazão volumétrica-->
    <string name="v1">Calcular usando:\n Q = V/∆t</string>
    <string name="v2">Calcular usando:\n Q = A × v</string>
    <string name="v3">Calcular usando:\n Q = π × r² × v</string>
    <string name="t1">Calcular usando:\n ∆t = V/Q</string>
    <string name="V1">Calcular usando:\n V = ∆t × Q</string>
    <string name="vel1">Calcular usando:\n v = Q/A</string>
    <string name="vel2">Calcular usando:\n v = Q / (π × r²)</string>
    <string name="area1">Calcular usando:\n A = Q/v</string>
    <string name="raio1">Calcular usando:\n r = √[Q / (π × v)]</string>

    <!--Cinemática MRU-->
    <string name="kinematic_mru_displacement1">Calcular usando:\n ∆S = Sf - Si</string>
    <string name="kinematic_mru_displacement2">Calcular usando:\n ∆S = ∆t × vm</string>
    <string name="kinematic_mru_displacement3">Calcular usando:\n ∆S = (tf - ti) × vm</string>
    <string name="kinematic_mru_initial_displacement">Calcular usando:\n Si = Sf - ∆S</string>
    <string name="kinematic_mru_final_displacement">Calcular usando:\n Sf = Si + ∆S</string>
    <string name="kinematic_mru_displacement_law">Calcular usando:\n S = Si + v × ∆t</string>
    <string name="kinematic_mru_speed1">Calcular usando:\n v = ∆S / ∆t</string>
    <string name="kinematic_mru_speed2">Calcular usando:\n v = (sf - si) × (tf - ti)</string>
    <string name="kinematic_mru_time1">Calcular usando:\n ∆t = tf - ti</string>
    <string name="kinematic_mru_time2">Calcular usando:\n ∆t = ∆S / v</string>
    <string name="kinematic_mru_time3">Calcular usando:\n ∆t = (Sf - Si) / v</string>
    <string name="kinematic_mru_initial_time">Calcular usando:\n ti = tf - ∆t</string>
    <string name="kinematic_mru_final_time">Calcular usando:\n tf = ti + ∆t</string>

    <!--Cinemática MUV-->
    <string name="kinematic_muv_acceleration1">Calcular usando:\n a = ∆v / ∆t</string>
    <string name="kinematic_muv_acceleration2">Calcular usando:\n a = (∆v) / (tf - ti)</string>
    <string name="kinematic_muv_acceleration3">Calcular usando:\n a = (vf - vi) / ∆t</string>
    <string name="kinematic_muv_acceleration4">Calcular usando:\n a = (vf - vi) / (tf - ti)</string>
    <string name="kinematic_muv_speed1">Calcular usando:\n ∆v = ∆t × a</string>
    <string name="kinematic_muv_speed2">Calcular usando:\n ∆v = (tf - ti) × a</string>
    <string name="kinematic_muv_speed3">Calcular usando:\n vi = vf - ∆t × a</string>
    <string name="kinematic_muv_speed4">Calcular usando:\n vf = vi + ∆t × a</string>
    <string name="kinematic_muv_time1">Calcular usando:\n ∆t = ∆v ÷ a</string>
    <string name="kinematic_muv_time2">Calcular usando:\n ∆t = (vf - vi) ÷ a</string>
    <string name="kinematic_muv_time3">Calcular usando:\n ti = tf - ∆v ÷ a</string>
    <string name="kinematic_muv_time4">Calcular usando:\n ti = tf - (vf - vi) ÷ a</string>
    <string name="kinematic_muv_time5">Calcular usando:\n tf = ti + ∆v ÷ a</string>
    <string name="kinematic_muv_time6">Calcular usando:\n tf = ti + (vf - vi) ÷ a</string>
    

